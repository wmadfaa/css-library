{
  "extends": "stylelint-config-recommended",
  "plugins": [
    "stylelint-scss"
  ],
  "rules": {
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "extends",
          "use",
          "forward",
          "include",
          "mixin",
          "each",
          "function",
          "return",
          "if",
          "error"
        ]
      }
    ],
    "block-no-empty": null
  }
}